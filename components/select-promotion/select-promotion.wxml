<view class="selecter-time-conainer" wx:if="{{isShow}}" catchtouchmove>
  <view style="height:65vh;width:100%;" catchtap="close"/>
  <view class="selecter-time-box">
    <view class="promo-box" wx:for="{{data}}" wx:key="index" bindtap="onParentEvent" data-promotion="{{item.promotionNo}}">
      <view class="tag">{{item.name}}</view>
      <view class="info-box">
        <view 
          class="info" 
          wx:for="{{item.msg}}" 
          wx:for-item="t"
          wx:for-index="zIndex"
          wx:key="zIndex" 
        >
          {{zIndex+1}}. {{t}}
        </view>
      </view>
      
    </view>
    <!-- <view class="btn-box">
      <text catchtap="close">取消</text>
      <text catchtap='come'>确定</text>
    </view> -->
  </view>
</view>
