<view class="time-box">
  <view class="start">
    <view>开始时间：</view>
    <text>{{startDate}}</text>
  </view>
  <view class="end">
    <view>结束时间：</view>
    <text>{{endDate}}</text>
  </view>
</view>
<input bindconfirm="confirm" placeholder="请输入订单号" class="input" value="{{searchValue}}" bindinput="bindValue">
  <image src="../../images/hide_giftInfo.png"></image>
</input>
