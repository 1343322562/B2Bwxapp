Page{background-color: #fff;}
.search{background-color: #fff;display: flex;height: 90rpx;position: fixed;left: 0;top: 0;width: 100%;border-bottom: 1px solid #e8e8e8;z-index: 9999;}
.search>.item{width: 0;}
.search>.form{flex-grow: 4;position: relative;background-color: #f2f2f2;height: 70rpx;border-radius: 15rpx;margin-left: 20rpx}
.search>.form .icon{width: 42rpx;height: 42rpx;position: absolute;top: 15rpx;left: 15rpx;}
.search>.form input{width: 70%;margin-left: 80rpx;height: 100%;}
.search>.quit{flex-grow: 1;text-align: center;line-height: 70rpx;}
.search .openCode{position: absolute;top: 0;right: 10rpx;height: 70rpx;width: 80rpx;text-align: center;}
.search .openCode>.code{width: 50rpx;height: 50rpx;margin-top: 12rpx;}
.searchKey{padding-top: 100rpx}
.keyList{position: relative;padding: 20rpx 20rpx;}
.keyList>.clear{width: 80rpx;height: 80rpx;position: absolute;top: 20rpx;right: 20rpx;text-align: center;}
.keyList>.clear .icon{width: 38rpx;height: 38rpx;}
.keyList>.list{padding: 20rpx  0;overflow: hidden}
.keyList>.list .li{background-color: #f2f2f2;color: #717171;float: left;padding: 10rpx  20rpx;margin-right: 20rpx;font-size: 26rpx;margin-top: 20rpx}
/*商品列表*/
.goodsList{margin-left: 20rpx;background-color: #fff;padding-left: 20rpx;padding-top: 100rpx;}
.goodsList>.li{padding: 20rpx 20rpx 20rpx 0;position: relative;border-bottom: 1px solid #e8e8e8;}
.goodsList>.li .img{position: absolute;top: 20rpx;left:0;overflow: hidden;width: 170rpx;height: 170rpx;background-color: #f2f2f2}
.goodsList>.li .img>.goodsType{color: #fff;background-color: #dd4242;position: absolute;top: 0;left: 20%;width: 60%;text-align: center;line-height:40rpx;font-size: 23rpx;border-radius: 0 0 10rpx 10rpx;}
.goodsList>.li .img>.icon{width: 100%;height: 100%}
.goodsList>.li .info{min-height: 170rpx;margin-left: 190rpx}
.goodsList>.li .info>.name{display: -webkit-box; word-break: break-all; -webkit-box-orient: vertical; -webkit-line-clamp:2; overflow: hidden; text-overflow:ellipsis;font-weight: 500;}
.goodsList>.li .info>.size{font-size: 26rpx;color: #888}
.goodsList>.li .info>.price{color: red;font-size: 32rpx}
.goodsList>.li .info>.price .before{margin-left: 10rpx;text-decoration:line-through;font-size: .8em;color: #999}
.goodsList>.li .info>.tag text{display: inline-block;line-height: 30rpx;font-size: 22rpx;border: 1px solid red;color: red;margin-left: 10rpx;border-radius: 10rpx;padding: 0 6rpx;}
.goodsList>.li .info>.productionTime{font-size: 22rpx;color: #666;}
.goodsList>.li .goCarts{position: absolute;right: 0;bottom: 20rpx;width: 190rpx}
.goodsList>.li .goCarts>.type{text-align: right;font-size: 24rpx;padding-right: 18rpx}
.goodsList>.li .goCarts>.type.cw{color: #ffb400}
.goodsList>.li .goCarts>.type.dw{color:#04a1ff;}
.goodsList>.li .goCarts>.btn{position: relative;height: 80rpx;text-align: center}
.goodsList>.li .goCarts>.btn .item{height: 100%;width: 80rpx;position: absolute;top: 0}
.goodsList>.li .goCarts>.btn .item>.icon{width: 50rpx;height: 50rpx;margin-top: 15rpx;background-color:#fff;}
.goodsList>.li .goCarts>.btn .minus{left: 0}
.goodsList>.li .goCarts>.btn .num{display: inline-block;line-height:80rpx}
.goodsList>.li .goCarts>.btn .add{right: 0}
.goCartsPage{text-align: center;position: fixed;z-index: 10;bottom:180rpx;right: 50rpx;width: 100rpx;height: 100rpx;border-radius: 100%;background-color: #fff;border:1px solid #e4e4e4;box-shadow: 5rpx 5rpx 10rpx #ddd;color: #777;font-size: 20rpx;}
.goCartsPage>.icon{width: 50rpx;height: 50rpx;margin-top: 10rpx;}
.goCartsPage>text{display: block;margin-top: -5rpx;}

.minSupplyQty{position: absolute;bottom: 47rpx;right: 81rpx;font-size: 24rpx;background-color: #fff;color:#04a1ff }

.noReturn {
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  background-color: #333333;
  white-space: nowrap;
  color: rgb(230, 187, 49);
  font-size: 19rpx;
  letter-spacing: 2rpx;
  padding: 8rpx 10rpx;
}

/* 直配商品列表*/
.z_goodsList{padding-top: 100rpx;}