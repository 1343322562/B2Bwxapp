Page{background: #fff}
.headImg{width:100%;padding-top:78%;background:url("http://zksrimg.oss-cn-beijing.aliyuncs.com/images/wxmini/getCupHeadImg.png")no-repeat center top;background-size:100%;}
.title{color:#df002c;padding:0 20rpx;font-size:28rpx;}
.list{text-align:center;margin-top:30rpx;}
.list>.li{width:40%;padding-top:18%;background:url("http://zksrimg.oss-cn-beijing.aliyuncs.com/images/wxmini/getCupBox.png")no-repeat center top;background-size:100%;display:inline-block;margin:10rpx 10rpx 0 10rpx;position:relative;}
.list .cnt{position:absolute;top:20rpx;left:30rpx;text-align:left;color:#df002c;font-size:24rpx;}
.list .cnt>.money{font-size:1.5em;}
.bottom{background-color:#df002c;border-radius:20rpx;padding-top:20rpx;margin:20rpx auto;min-height:400rpx;width:95%;color:#fff;padding-bottom:20rpx;}
.bottom .cnt{margin:20rpx auto 0 auto;width:92%;}
.topTitle{width:400rpx;height:75rpx;margin:0 auto;text-align:center;color:#fff;background:url('http://zksrimg.oss-cn-beijing.aliyuncs.com/images/wxmini/getCupInfoTitle.png')no-repeat center top;background-size:100%;font-size:30rpx;line-height:62rpx;}.data-null{text-align:center;color:#df002c;font-size:28rpx;margin:40rpx 0;}
.list>.li.act {filter: gray;filter: grayscale(100%);}
